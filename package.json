    "interpret": "^1.0.0",
    "liftoff": "^2.1.0",
    "matchdep": "^1.0.0",
    "mute-stdout": "^1.0.0",
    "pretty-hrtime": "^1.0.0",
    "semver-greatest-satisfied-range": "^1.0.0",
    "tildify": "^1.0.0",
    "v8flags": "^2.0.9",
    "wreck": "^10.0.0",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "code": "^4.0.0",
    "coveralls": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-gulp": "^2.0.0",
    "fs-extra": "^2.0.0",
    "gulp": "gulpjs/gulp#4.0",
    "jscs": "^3.0.7",
    "jscs-preset-gulp": "^1.0.0",
    "lab": "^11.2.2",
    "marked-man": "^0.2.0"
