{
  "name": "gulp-cli",
  "version": "1.1.0",
  "description": "Command line interface for gulp",
  "author": "Gulp Team <team@gulpjs.com> (http://gulpjs.com/)",
  "contributors": [],
  "homepage": "http://gulpjs.com",
  "repository": "gulpjs/gulp-cli",
  "license": "MIT",
  "man": "gulp.1",
  "engines": {
    "node": ">= 0.10"
  },
  "main": "index.js",
  "bin": {
    "gulp": "bin/gulp.js"
  },
  "files": [
    "index.js",
    "lib",
    "bin",
    "completion",
    "gulp.1"
